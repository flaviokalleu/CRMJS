  "main": "index.js",
  "scripts": {
    "build": "npm run obfuscate && npm run postbuild",
    "obfuscate": "javascript-obfuscator src --output dist",
    "migrate": "sequelize db:migrate --config src/config/database.js --migrations-path src/migrations",
    "seed": "sequelize db:seed:all --config src/config/database.js --seeders-path src/seeders",
    "postbuild": "copyfiles -u 1 'src/server.js' '.env' dist",
    "start": "node dist/server.js",
    "st": "node src/server.js",
    "init": "npm run migrate && npm run seed"
  },
